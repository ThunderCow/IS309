  size 32M
  autoextend on
  next 32M
  extent management local
  UNIFORM SIZE 4M;
create tablespace OHAMA64MB
  logging
  datafile '/u01/app/oracle/oradata/WOLCOTTDB/OHAMA64MB.dbf'
  size 64M
  autoextend on
  next 64M
  extent management local
